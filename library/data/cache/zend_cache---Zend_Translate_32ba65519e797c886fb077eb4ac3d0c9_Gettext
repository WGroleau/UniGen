ots.txt file.";s:161:"To use a Google mail account, use the following settings: server=smtp.gmail.com, port=587, security=tls, username=xxxxx@gmail.com, password=[your gmail password]";s:161:"To use a Google mail account, use the following settings: server=smtp.gmail.com, port=587, security=tls, username=xxxxx@gmail.com, password=[your gmail password]";s:4:"Togo";s:4:"Togo";s:7:"Tokelau";s:7:"Tokelau";s:12:"Tokyo, Japan";s:12:"Tokyo, Japan";s:9:"Tombstone";s:9:"Tombstone";s:5:"Tonga";s:5:"Tonga";s:17:"Top %s given name";a:2:{i:0;s:17:"Top %s given name";i:1;s:18:"Top %s given names";}s:18:"Top %s given names";s:0:"";s:14:"Top %s surname";a:2:{i:0;s:14:"Top %s surname";i:1;s:15:"Top %s surnames";}s:15:"Top %s surnames";s:0:"";s:9:"Top Level";s:9:"Top Level";s:14:"Top given name";s:14:"Top given name";s:15:"Top given names";s:15:"Top given names";s:11:"Top surname";s:11:"Top surname";s:12:"Top surnames";s:12:"Top surnames";s:24:"Toronto, Ontario, Canada";s:24:"Toronto, Ontario, Canada";s:24:"Total accepted changes: ";s:24:"Total accepted changes: ";s:12:"Total births";s:12:"Total births";s:10:"Total dead";s:10:"Total dead";s:12:"Total deaths";s:12:"Total deaths";s:14:"Total divorces";s:14:"Total divorces";s:12:"Total events";s:12:"Total events";s:14:"Total families";s:14:"Total families";s:18:"Total families: %s";s:18:"Total families: %s";s:13:"Total females";s:13:"Total females";s:17:"Total given names";s:17:"Total given names";s:17:"Total individuals";s:17:"Total individuals";s:21:"Total individuals: %s";s:21:"Total individuals: %s";s:12:"Total living";s:12:"Total living";s:11:"Total males";s:11:"Total males";s:15:"Total marriages";s:15:"Total marriages";s:21:"Total number of users";s:21:"Total number of users";s:23:"Total pending changes: ";s:23:"Total pending changes: ";s:16:"Total places: %s";s:16:"Total places: %s";s:17:"Total sources: %s";s:17:"Total sources: %s";s:14:"Total surnames";s:14:"Total surnames";s:11:"Total users";s:11:"Total users";s:7:"Trailer";s:7